    "@nx/js": "22.5.2",
    "@swc-node/register": "~1.11.1",
    "@swc/core": "~1.15.5",
    "@swc/helpers": "~0.5.18",
    "@types/jest": "^30.0.0",
    "@types/node": "20.19.9",
    "@types/react": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-preset-expo": "~54.0.7",
    "eslint": "^8.57.0",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",

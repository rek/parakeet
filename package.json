{
  "name": "parakeet",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "typecheck": "nx affected --target=typecheck",
    "lint": "nx affected --target=lint",
    "test": "nx affected --target=test",
    "mobile:start": "nx start mobile",
    "mobile:ios": "nx run-ios mobile",
    "mobile:android": "nx run-android mobile",
